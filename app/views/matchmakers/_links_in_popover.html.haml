- if user.user_status == 'enabled'
  =link_to 'View Profile', fetch_user_path(user_id: user.id), {"data-target" => "#profileModal", "data-toggle" => "modal", remote: true}
  %br
  - if authenticated_user.display_name == user.display_name
  - else
    =link_to 'Send Message', new_conversation_path(recipient_id: user.id), target: '_blank'
